sudo apt update
sudo apt install -y qemu qemu-kvm libvirt-daemon-system libvirt-clients bridge-utils virt-manager qemu-utils


egrep -c '(vmx|svm)' /proc/cpuinfo

sudo apt update
sudo apt install -y cloud-image-utils

which cloud-localds
cloud-localds --version

sudo mkdir -p /root/vm

sudo apt update
sudo apt install -y cloud-image-utils

cd /root/vm
sudo cloud-localds seed.iso user-data meta-data

touch meta-data

ls -lh /root/vm/seed.iso

qemu-system-x86_64 -drive file=/root/vm/seed.iso,format=raw,if=virtio ...
